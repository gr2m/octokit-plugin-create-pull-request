{
  "name": "octokit-plugin-create-pull-request",
  "version": "0.0.0-development",
  "description": "Octokit plugin to create a pull request with multiple file changes",
  "type": "module",
  "scripts": {
    "build": "node scripts/build.mjs && tsc -p tsconfig.json",
    "lint": "prettier --check \"{src,test}/**/*.{ts,md}\" *.md package.json tsconfig.json",
    "lint:fix": "prettier --write \"{src,test}/**/*.{ts,md}\" *.md package.json tsconfig.json",
    "pretest": "npm run -s lint",
    "test": "vitest run --coverage"
  },
  "keywords": [
    "github",
    "octokit",
    "plugin",
    "pull-request"
  ],
  "author": "Gregor Martynus (https://github.com/gr2m)",
  "license": "MIT",
  "dependencies": {
    "@octokit/types": "^13.5.0"
  },
  "devDependencies": {
    "@octokit/core": "^6.0.0",
    "@octokit/plugin-paginate-rest": "^11.0.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^4.0.10",
    "esbuild": "^0.23.0",
    "glob": "^11.0.0",
    "prettier": "3.3.0",
    "semantic-release-plugin-update-version-in-files": "^1.1.0",
    "typescript": "^5.5.3",
    "vitest": "^2.0.2"
  },
  "repository": "github:gr2m/octokit-plugin-create-pull-request",
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "./pkg"
        }
      ],
      [
        "semantic-release-plugin-update-version-in-files",
        {
          "files": [
            "pkg/dist-web/*",
            "pkg/dist-node/*",
            "pkg/*/version.*"
          ]
        }
      ]
    ]
  },
  "engines": {
    "node": ">=18"
  }
}
